<?xml version="1.0" ?>
<hibernate-mapping  auto-import="true"
										default-lazy="true"
										xmlns:xsd="http://www.w3.org/2001/XMLSchema"
										xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
										xmlns="urn:nhibernate-mapping-2.2">
  <class name="AMS.Broker.AutherizationService.DataStore.User, AMS.Broker.AutherizationService"
				 table="[User]">
    <cache usage="read-write" />
    <id name="UserId"
				access="property"
				column="UserId"
				type="Int32"
				unsaved-value="0">
      <generator class="native">
      </generator>
    </id>
    <property name="UserSid"
							access="property"
							type="String">
      <column name="UserSid"
							not-null="true"/>
    </property>
    <property name="UserTypeId"
							access="property"
							type="Int32">
      <column name="UserTypeId"
							not-null="false"/>
    </property>
    <property name="PersonId"
							access="property"
							type="Int32">
      <column name="PersonId"
							not-null="true"/>
    </property>
    <property name="StatusCode"
							access="property"
							type="Int32">
      <column name="StatusCode"
							not-null="true"/>
    </property>
  </class>
</hibernate-mapping>